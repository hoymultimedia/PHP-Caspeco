{
    "name": "schimpanz/caspeco",
    "description": "A Caspeco API library with Laravel support",
    "license": "MIT",
    "keywords": ["caspeco", "payments", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "vincent@schimpanz.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "sebastian/peek-and-poke": "^1.0",
        "graham-campbell/testbench": "^3.0",
        "graham-campbell/manager": "^2.2",
        "mockery/mockery": "^0.9.4",
        "illuminate/contracts": "5.1.*",
        "illuminate/support": "5.1.*"
    },
    "suggest": {
        "illuminate/support": "Required to integrate with Laravel (^2.2).",
        "graham-campbell/manager": "Required to integrate with Laravel (^3.0)."
    },
    "autoload": {
        "psr-4": {
            "Schimpanz\\Caspeco\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schimpanz\\Tests\\Caspeco\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
