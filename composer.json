{
    "name": "hoy/caspeco",
    "description": "A Caspeco API library with Laravel support",
    "license": "MIT",
    "keywords": ["caspeco", "payments", "laravel", "hoy"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "vincent@hoy.se"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/http-kernel": "^2.8 || ^3.0",
        "danielstjules/stringy": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "graham-campbell/testbench": "^3.2",
        "graham-campbell/manager": "^2.4",
        "mockery/mockery": "^0.9.5",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "suggest": {
        "illuminate/support": "Required to integrate with Laravel (^5.2).",
        "graham-campbell/manager": "Required to integrate with Laravel (^3.1)."
    },
    "autoload": {
        "psr-4": {
            "Hoy\\Caspeco\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hoy\\Tests\\Caspeco\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
